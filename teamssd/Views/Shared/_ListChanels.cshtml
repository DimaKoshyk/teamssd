@model List<teamssd.Data.Entities.Chanel>

 @foreach (var chanel in Model.ToList())
 {
     <div class="list-group-item" style="overflow: inherit;">
         <div class="row-picture">
             <img class="circle" src="http://lorempixel.com/56/56/people/1" alt="icon">
         </div>
         <div class="row-content">
             <h4 class="list-group-item-heading get-news-chanel" data-id="@chanel.Id">@chanel.Name</h4>
             <p class="list-group-item-text">Donec id elit non mi porta gravida at eget metus</p>
         </div>
         <span class="glyphicon glyphicon glyphicon-option-vertical more-chanel dropdown-toggle" aria-hidden="true" id="dropdownMenu1" data-toggle="dropdown" ></span>
         <ul class="dropdown-menu" aria-labelledby="dropdownMenu1" style="top: 16px;left: 58px;">
             <li><a href="#">Action</a></li>
             <li><a href="#">Another action</a></li>
             <li><a href="#">Something else here</a></li>
             <li role="separator" class="divider"></li>
             <li><a href="#">Separated link</a></li>
         </ul>
     </div>
     if (chanel != Model.Last())
     {
         <hr/>
     }
                        
 }